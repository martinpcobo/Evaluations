        <script><![CDATA[(function () {
	/* populate the 'data' object */
	/* e.g., data.table = $sp.getValue('table'); */
	data.valid = true;

	var quiz_gr = new GlideRecord('asmt_assessment_instance');
	if (!quiz_gr.get($sp.getParameter('sys_id'))) {
		data.valid = false;
		return;
	}
	
	console.log(new NewJoinerUtils().getMetricInstanceDetails(quiz_gr.getUniqueValue()).categories);
	data.categories = new NewJoinerUtils().getMetricInstanceDetails(quiz_gr.getUniqueValue()).categories;
})();
]]></script>
